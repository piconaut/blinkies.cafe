<%- include('components/head.ejs', {
    path: "feed", 
    title: "the blinkie feed | recently created blinkies!", 
    description: "a feed of the 18 latest user-created blinkies."
})%>
<div class='content'>
    <h1>recently generated blinkies</h1>
</div>
<%
    if (warn) {
%>
        <div class='content'>
            this page hosts the most recent generated blinkies, with user-created text which blinkies.cafe may not endorse. please be kind &lt;3<br><br>
            <center><b>&gt;&nbsp;<a href='/feed'>ENTER</a>&nbsp;&lt;</b></center>
        </div>
<%
    }
    else {
%>
        <div class='content'>
            click any blinkie to customize!
            <br><br>
            <center>
            <a href='/feed'>refresh</a>
            <div id='gallery' type='text' style='position:relative; z-index:0; visibility:'>
                <center id='tiles'>
                    <%
                    for (const blinkie of recentBlinkies) {
                        %><a href="/?s=<%=blinkie.style%>"><img src="/b/blinkiesCafe-<%=blinkie.blinkieID%>.gif" class="blinkie"></a><%
                    }
                    %>
                </center>
            </div>
            </center>
        </div>
        <div class='content smalltext'>
            this page hosts the most recent generated blinkies, with user-created text which blinkies.cafe may not endorse. please be kind &lt;3
        </div>
<%
    }
%>
