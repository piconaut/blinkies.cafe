<%- include('components/head.ejs', {
    path: "submitters", 
    title: `blinkie template submitters`, 
    description: `blinkie template credits -- all the blinkies.cafe customizable blinkies, sorted by submitter.`
})%>
<div class='content'>
    <h1>blinkie template submitters</h1>
</div>
<div class='content'>
    <div id='gallery' type='text' style='position:relative; z-index:0; visibility:'>
        <a href="/">&#60; back to gallery</a><br><br>
        all the customizable blinkie templates, sorted by submitter!<br><br>
        huge thank you to all blinkies.cafe community members who've contributed!! :D<br><br>

        click a blinkie to customize c:<br><br>
        <center id='tiles'>
<%
            const blinkieExt = freeze ? 'png' : 'gif'
            for (const [subID, subData] of Object.entries(submitters)) {
%>
                <h2 id="<%-subID%>"><a href="<%=subData.subURL%>"><%-subData.subName%></a></h2><br>
                    
<%              
                for (const key of subData.templates) {
                    %><a rel="nofollow" href="https://blinkies.cafe/?s=<%=key%>"><img src="/b/display/<%=key%>.<%=blinkieExt%>" alt="<%=key%> blinkie" loading="lazy" class="blinkie blinkieTile"></a><%
                }
%>
                <br><br><hr><br><br>
<%        
            }   
%>
        </center>
    </div>
</div>
<%- include('components/footer.html') %>
